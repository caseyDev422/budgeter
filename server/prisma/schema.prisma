generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model item {
  id           Int     @id @default(autoincrement())
  name         String
  amount       Float
  dueDate      String
  hasAutoDraft Boolean
}

model user {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  updatedAt    DateTime
  name         String
  occupation   String
  income       Float
}
